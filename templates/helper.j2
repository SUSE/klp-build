{% if ktype == 'klp' %}
{% set fname = 'livepatch_' + bsc %}
{% else %}
{% set fname = 'kgr_patch_' + bsc %}
{% endif %}

{% macro get_commits(codestream) -%}
{% for commit, msg in commits[codestream].items() -%}
{% if codestream == 'upstream' %}
 *  {{ commit }} ("{{ msg }}")
{% else %}
 *  {{ commit }}
{% endif -%}
{% endfor -%}
{% endmacro -%}
