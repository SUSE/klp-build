{% macro get_commits(codestream) %}
{% if not commits[codestream] %}
 *  Not affected
{% else %}
{% for commit, msg in commits[codestream].items() %}
{% if codestream == 'upstream' %}
 *  {{ commit }} ("{{ msg }}")
{% else %}
{% for cmt in msg %}
 *  {{ cmt }}
{% endfor %}
{% endif %}
{% endfor %}
{% endif %}
 *
{%- endmacro %}
